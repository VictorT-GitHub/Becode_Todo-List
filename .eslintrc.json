{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
